// Test file to verify type alias method validation

// Valid: struct type alias should allow methods
type Point struct {
    x: i32,
    y: i32
};

type MyPoint Point;

// This should be VALID - MyPoint is an alias for a struct
fn (p: MyPoint) distance() -> f64 {
    return 0.0;
}

// Invalid: primitive type alias should NOT allow methods
type MyInt i32;

// This should be INVALID - MyInt is an alias for a primitive
fn (i: MyInt) double() -> i32 {
    return i * 2;
}

// Invalid: array type alias should NOT allow methods
type MyArray []i32;

// This should be INVALID - MyArray is an alias for an array
fn (a: MyArray) sum() -> i32 {
    return 0;
}

fn main() -> i32 {
    return 42;
}
