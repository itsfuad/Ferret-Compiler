// Test field and method access
type Person struct{
    name: str,
    age: i32
};

fn (self: Person) getName() -> str {
    return self.name;
}

fn (self: Person) getAge() -> i32 {
    return self.age;
}

fn main() {
    let p: Person = @Person{
        name: "Alice",
        age: 30
    };
    
    // Test field access
    let name: str = p.name;
    let age: i32 = p.age;

    // Test method access
    let nameMethod: str = p.getName();
    let ageMethod: i32 = p.getAge();
}
