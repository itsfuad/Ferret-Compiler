// Test more detailed struct casting error scenarios
type Point struct {
    x: i32,
    y: i32
};

type ColorPoint struct {
    x: str,  // Different type from Point.x (i32)
    y: i32,
    color: str
};

fn main() {
    // Test 1: Anonymous struct missing fields
    let anonStruct = @struct{x: 10};
    let point: Point = anonStruct as Point; // Missing 'y' field
    
    // Test 2: Type mismatch (x is str instead of i32)
    let colorPoint: ColorPoint = @ColorPoint{x: "hello", y: 20, color: "red"};
    let invalidPoint: Point = colorPoint as Point; // x type mismatch
}
