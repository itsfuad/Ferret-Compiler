// Test invalid struct casting cases
type Point struct {
    x: i32,
    y: i32
};

type Person struct {
    name: str,
    age: i32
};

fn main() {
    let point: Point = @Point{x: 10, y: 20};
    let person: Person = @Person{name: "Alice", age: 30};
    
    // This should fail - Point and Person have different fields
    let invalidCast: Person = point as Person;
}
