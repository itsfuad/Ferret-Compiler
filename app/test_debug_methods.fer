// Debug method collection
type TestStruct struct {
    value: i32
};

fn (self: TestStruct) method1() -> i32 {
    return self.value;
}

fn (self: TestStruct) method2() -> i32 {
    return self.value * 2;
}

fn main() {
    let ts: TestStruct = @TestStruct{value: 5};
    
    // Simple field access should work
    let v: i32 = ts.value;
    
    // Method calls should work  
    let m1: i32 = ts.method1();
    let m2: i32 = ts.method2();
}
