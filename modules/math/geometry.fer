import "compiler/math/constants";
// Math module with geometric functions

fn area_circle(radius: f64) -> f64 {
    return constants::PI * radius * radius;
}

fn area_rectangle(width: f64, height: f64) -> f64 {
    return width * height;
}

fn perimeter_circle(radius: f64) -> f64 {
    return 2.0 * constants::PI * radius;
}
